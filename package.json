    "prod:build:server": "cross-env NODE_ENV=production webpack --config webpack/webpack.server.config.common.ts",
    "prod:launch:server": "node ./build/server/index.js",
    "prod:run:server": "run-s prod:build:client-ssr prod:build:server prod:launch:server",
