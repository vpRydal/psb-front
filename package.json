    "dev:build:client": "cross-env NODE_ENV=development webpack --config webpack/webpack.client.config.dev.ts",
    "dev:run:client": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.client.config.dev-server.ts",
    "dev:run:server": "cross-env NODE_ENV=development nodemon ./_server/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx _client _server webpack --fix"
  },
